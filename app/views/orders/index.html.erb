<h1>Все заказы</h1>
<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>Имя</th>
    <th>Email</th>
    <th>Телефон</th>
  </tr>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.name %></td>
      <td><%= order.email %></td>
      <td><%= order.phone %></td>
    </tr>
  <% end %>
  </tbody>
</table>